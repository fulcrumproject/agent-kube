{
  "apiVersion": "kamaji.clastix.io/v1alpha1",
  "kind": "TenantControlPlane",
  "metadata": {
    "name": "tenant-test",
    "labels": {
      "tenant.clastix.io": "tenant-test"
    }
  },
  "spec": {
    "controlPlane": {
      "deployment": {
        "replicas": 2
      },
      "service": {
        "serviceType": "LoadBalancer"
      }
    },
    "kubernetes": {
      "version": "v1.30.0",
      "kubelet": {
        "cgroupfs": "systemd"
      }
    },
    "networkProfile": {
      "port": 6443
    },
    "addons": {
      "coreDNS": {},
      "kubeProxy": {},
      "konnectivity": {
        "server": {
          "port": 8132
        }
      }
    }
  }
}